{
    "version": "0.2",
    "dictionaries": [
        "custom-dictionary"
    ],
    "useGitignore": true,
    "ignorePaths": [
        ".git",
        "_raw_assets",
        "*.svg",
        "docs/assets/images",
        "Gemfile.lock"
    ],
    "dictionaryDefinitions": [
        {
            "name": "custom-dictionary",
            "path": "./.cspell/custom-dictionary.txt",
            "addWords": true
        }
    ],
    "enableFiletypes": [
        "ruby"
    ]
}
