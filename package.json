{
  "name": "street-side-software-site",
  "private": true,
  "description": "Website for Street Side Software",
  "scripts": {
    "build": "pnpm run -r build",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "spell-check": "pnpx cspell --dot .",
    "start": "pnpm run -r start --filter=website",
    "test": "pnpm build",
    "update": "pnpm up -w -r"
  },
  "devDependencies": {
    "prettier": "^3.8.1"
  },
  "packageManager": "pnpm@10.28.1"
}
